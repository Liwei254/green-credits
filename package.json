  "bugs": {
    "url": "https://github.com/Liwei254/green-credits/issues"
  },
  "homepage": "https://github.com/Liwei254/green-credits#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "dotenv": "^16.4.5",
    "hardhat": "^2.27.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "ethers": "^6.13.0"
  }
}
