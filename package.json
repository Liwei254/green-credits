{
  "name": "green-credits",
  "version": "1.0.0",
  "description": "A blockchain-based Green Credits project built with Hardhat, TypeScript, and the Moonbase Alpha test network.",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "clean": "npx hardhat clean",
    "test": "npx hardhat test",
    "deploy:moonbase": "npx hardhat run scripts/deploy.js --network moonbase",
    "deploy:moonbeam": "npx hardhat run scripts/deploy-moonbeam.ts --network moonbasealpha",
    "deploy:phase1": "npx hardhat run scripts/deploy_phase1.ts --network moonbase",
    "deploy:phase2": "npx hardhat run scripts/deploy_phase2.ts --network moonbase",
    "deploy:phase3": "npx hardhat run scripts/deploy_phase3.ts --network moonbase",
    "upsert:samples": "npx hardhat run scripts/upsert_samples.ts --network moonbase",
    "node": "npx hardhat node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liwei254/green-credits.git"
  },
  "keywords": [
    "hardhat",
    "moonbeam",
    "moonbase",
    "solidity",
    "web3",
    "green-credits"
  ],
  "author": "James Otipa",
